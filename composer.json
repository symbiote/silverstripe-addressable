{
    "name": "symbiote/silverstripe-addressable",
    "description": "SilverStripe addressable and geocoding module",
    "type": "silverstripe-vendormodule",
    "keywords": [
        "silverstripe",
        "symbiote",
        "addressable",
        "geocoding"
    ],
    "license": "BSD-3-Clause",
    "authors": [
        {
            "name": "Symbiote",
            "homepage": "https://www.symbiote.com.au/"
        },
        {
            "name": "Marcus Nyeholt",
            "email": "nyeholt@symbiote.com.au"
        }
    ],
    "require": {
        "silverstripe/framework": "^4.12||^5",
        "guzzlehttp/guzzle": "^7"
    },
    "require-dev": {
        "squizlabs/php_codesniffer": "^3.5",
        "phpunit/phpunit": "^9.6"
    },
    "scripts": {
        "phpcbf": "phpcbf src/ src/ tests/",
        "phpcs": "phpcs src/ src/ tests/"
    },
    "autoload": {
        "psr-4": {
            "Symbiote\\Addressable\\": "src/",
            "Symbiote\\Addressable\\Tests\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "5.1.x-dev"
        }
    },
    "replace": {
        "ajshort/silverstripe-addressable": "self.version",
        "silverstripe-australia/addressable": "self.version"
    },
    "prefer-stable": true,
    "minimum-stability": "dev"
}
